# http://editorconfig.org
# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
# Set default charset
[*]
charset = utf-8
trim_trailing_whitespace = true
end_of_line = lf
insert_final_newline = true

# Space indentation
[{*.js,*.json,*.yml}]
indent_size = 2
indent_style = space
